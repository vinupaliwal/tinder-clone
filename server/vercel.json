{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      },
      {"src":"**/*.js","use":"@now/node"},
      {"src":"images/**/*","use":"@now/static"}
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }